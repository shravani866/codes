int main() {
    int N, num, sum=0, digits=0;
    scanf("%d", &N);
    num=N;

    int temp=N;
    while (temp!=0) {
        temp/=10;
        digits++;
    }
    temp=N;
    int pos=digits;
    sum=0;

    while(temp!=0) {
        int digit=temp/pow(10,pos-1);
        sum+=pow(digit,digits-pos+1);

        temp=temp%(int)pow(10,pos-1);
        pos--;
    }
    if(sum==N)
        printf("True\n");
    else
        printf("False\n");
    return 0;
}
