#include<stdio.h>
int mod_pow(int x,int y,int m)
{
    int result=1;
    x=x%m;
    while(y>0){
        if(y%2==1){
            result=(result*x)%m;
        }
        y=y/2;
        x=(x*x)%m;
    }
    return result;
}

int main()
{
    int x,y,m;
    scanf("%d %d %d",&x,&y,&m);
    printf("%d
",mod_pow(x,y,m));
    return 0;
}
