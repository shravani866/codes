#include<stdio.h>
void sort_desc(int*a, int n)
{
    for(int i=0;i<n-1;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(a[i]<a[j])
            {
                int temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
}
int main()
{
    int A[3], B[3];
    scanf("%d %d %d %d %d %d",&A[0],&A[1],&A[2],&B[0],&B[1],&B[2]);
    sort_desc(A,3);
    sort_desc(B,3);
    int alice=A[0]+A[1];
    int bob=B[0]+B[1];
    if(alice>bob)
        printf("Alice
");
    else if(bob>alice)
printf("Bob
");
else
printf("Tie
");
return 0;
}
